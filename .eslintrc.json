{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": ["prettier"],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "rules": {
        "prettier/prettier": [
            "error",
            {
                "singleQuote": true,
                "parser": "flow",
                "endOfLine": "auto"
            }
        ],
        "camelcase": "error",
        "eqeqeq": ["error", "always"]
    }
}
